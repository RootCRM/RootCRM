<div id="topWrap">
    <!-- TESTING 13:21 18Feb2011 -->
    <div id="top">
    
 <div id="search">
        <form id="searchForm" method="get" action="search" onsubmit="return $.checkSearchBox();">
          <fieldset>
          <p>
            <input type="text" name="s" class="required" value="" id="csearch" />
          </p>
          </fieldset>
          <p>
            <button class="submit" type="submit">Send</button>
          </p>
        </form>
      </div>
      <!-- end search -->
      <div id="miscMenu">
        <ul>
        	<% if (typeof navigation !== 'undefined' && navigation !== null) {
				for(var tnav=0; tnav<navigation.length; tnav++){
					if(navigation[tnav].categories=='top-navigation'){ 
						if(navigation[tnav].type=="url"){		%>
						 <li><a HREF="<%= navigation[tnav].content %>" title="<%= navigation[tnav].label %>"><span><%= navigation[tnav].label %></span></a></li>
			<%			}else if(navigation[tnav].type=="items"){
							var bookmarkItems = JSON.parse(navigation[tnav].bookmark_items);
							if(bookmarkItems.length>0){	%>
							<li>
								<a  href="#"><span><%= navigation[tnav].label %></span></a>
								 <ul>
								 	<% bookmarkItems.sort(dynamicSort("item_sort_order"));
								 		
								 	for(var count=0; count< bookmarkItems.length; count++){
								 		if(bookmarkItems[count].type=="url"){  %>
								 		 <li><a href="<%= bookmarkItems[count].content %>" title="<%= bookmarkItems[count].label %>"><%= bookmarkItems[count].label %></a></li>
								 	<% 	}else if(bookmarkItems[count].type=="html"){  %>
								 			<%- bookmarkItems[count].content %>
								 	<%	}
								 	}	 %>
								 </ul>
							</li>
					<%		}
						}
					} 
				}	
			} %>
         
        </ul>
      </div>
      <!-- end miscMenu -->
      
		<!--LinkedIn Follow -->
		<div id="linkedInFollow" style="position:absolute;top:80px;right:74px; ">
			<script src="//platform.linkedin.com/in.js" type="text/javascript"></script>
			<script type="IN/FollowCompany" data-id="376890" data-counter="right"></script>
 		</div>
        <!--LinkedIn Follow -->

    </div>
    <!-- end top -->
    <div id="logo"><a href="/"><img src="http://static.jobshout.co.uk/images/logo_l.gif" alt="" /></a></div>
    <div id="mainMenu">
      <ul>
      	<% if (typeof navigation !== 'undefined' && navigation !== null) {
				for(var tnav=0; tnav<navigation.length; tnav++){
					if(navigation[tnav].categories=='main-menu'){ 
						if(navigation[tnav].type=="url"){		%>
						 <li><a HREF="<%= navigation[tnav].content %>" title="<%= navigation[tnav].label %>"><span><%= navigation[tnav].label %></span></a></li>
			<%			}else if(navigation[tnav].type=="items"){
							var bookmarkItems = JSON.parse(navigation[tnav].bookmark_items);
							if(bookmarkItems.length>0){	%>
							<li>
								<a  href="#"><span><%= navigation[tnav].label %></span></a>
								 <ul>
								 	<% bookmarkItems.sort(dynamicSort("item_sort_order"));
								 		
								 	for(var count=0; count< bookmarkItems.length; count++){
								 		if(bookmarkItems[count].type=="url"){  %>
								 		 <li><a href="<%= bookmarkItems[count].content %>" title="<%= bookmarkItems[count].label %>"><%= bookmarkItems[count].label %></a></li>
								 	<% 	}else if(bookmarkItems[count].type=="html"){  %>
								 		<li><%- bookmarkItems[count].content %></li>
								 	<%	}
								 	}	 %>
								 </ul>
							</li>
					<%		}
						}
					} 
				}	
			} %>
      </ul>
    </div>
    <!-- end mainMenu -->
  </div>
